
public class Main {

	public static void main(String[] args)
	{
		String filename = Constants.PATH_CONF_FILE;
		PropParser.load(filename);
		System.out.println("Beginning Program...");
		
		double[][] testNet = {
				{0,0,1},
				{0,0,1},
				{0,0,0}
		};
		double[] testThreshold = {0,0,.5};
		
		FeedForwardNeuralNet test = new FeedForwardNeuralNet(testNet, testThreshold, 2, 1);
		
//		double[] testInput = {1,0};
		test.printTruthTable01();
		NetVisualizer t = new NetVisualizer(test);
	}
	
}
